{
  "name": "nft-collection-gallery",
  "version": "0.2.1",
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build": "rollup -c -o dist/CollectionGallery.min.js",
    "link-nft-collection-gallery": "yarn link; cd example; yarn link nft-collection-gallery; cd ..",
    "link-react": "cd example/node_modules/react; yarn link; cd ../../../; yarn link react",
    "link-all": "yarn link-nft-collection-gallery; yarn link-react",
    "unlink-all": "yarn unlink nft-collection-gallery; yarn unlink react",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "np --no-tests"
  },
  "main": "dist/CollectionGallery.min.js",
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tansanDOTeth/nft-collection-gallery.git"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^10.4.8",
    "js-combinatorics": "^2.1.1",
    "np": "^7.6.2",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "rollup": "^2.78.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "webpack": "^5.74.0"
  }
}
